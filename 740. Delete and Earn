class Solution {
    public int deleteAndEarn(int[] nums) {
        int[] sum = new int[10001];
        for (int n : nums) 
            sum[n] += n;
        int prev = 0, curr = 0;
        for (int i = 0; i < sum.length; i++) {
            int temp = Math.max(curr, prev + sum[i]);
            prev = curr;
            curr = temp;
        }
        return curr;
    }
}
